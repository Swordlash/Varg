{
    "name": "varg-lang",
    "displayName": "Varg Lang",
    "description": "Support for Varg language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "varg",
                "aliases": [
                    "Varg",
                    "varg"
                ],
                "extensions": [
                    ".vg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "varg",
                "scopeName": "Main.vg",
                "path": "./syntaxes/varg.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    }
}
